{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["airbnb-base", "prettier"],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    "no-console": "off",
    "prefer-const": "off",
    "no-plusplus": "off",
    "no-underscore-dangle": "off",
    "dot-notation": "off",
    "no-unused-vars": "off"
  }
}
